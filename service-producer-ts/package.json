  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prod": "NODE_ENV=production node ./dist/server.js",
    "start": "npm run build-ts && node ./dist/index.js",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint --ext .js,.jsx,.ts,.tsx ./src",
    "dev": "ts-node-dev --trace-warnings --respawn --no-notify --files --ignore-watch node_modules src/server.ts",
    "build": "rm -rf dist && tsc -b --listEmittedFiles",
    "build-ts": "tsc -p .",
    "clean": "tsc -b --clean",
    "debug": "node --inspect -r ts-node/register src/server.ts",
    "lint:fix": "npm run lint -- --fix",
    "normalize-db": "npm run build-ts && node ./dist/commands/PopulateCalendars.js"
  },
